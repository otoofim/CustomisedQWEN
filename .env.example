# Qwen3.5 Virtual Try-On Pipeline Environment Configuration
# Copy this file to .env and customize the values for your setup
# Simple end-to-end pipeline using only Qwen3.5 multimodal model

# =============================================================================
# MODEL CONFIGURATION
# =============================================================================

# Qwen3.5 multimodal model for complete virtual try-on
QWEN_MODEL_ID=Qwen/Qwen3.5-397B-A17B

# Device to use for computation (cuda, cpu, or auto)
DEVICE=auto

# =============================================================================
# IMAGE PROCESSING SETTINGS
# =============================================================================

# Default image resolution for processing
IMAGE_RESOLUTION=512

# Random seed for reproducible results (leave empty for random)
RANDOM_SEED=

# =============================================================================
# QWEN GENERATION SETTINGS
# =============================================================================

# Maximum tokens for Qwen responses
MAX_NEW_TOKENS=2048

# Temperature for Qwen generation (0.0 to 1.0)
QWEN_TEMPERATURE=0.7

# =============================================================================
# FILE PATHS
# =============================================================================

# Input directories
PERSON_IMAGES_DIR=./input/person_images
GARMENT_IMAGES_DIR=./input/garment_images

# Output directories
OUTPUT_DIR=./outputs
RESULTS_DIR=./outputs/results
ANALYSIS_DIR=./outputs/analysis
COMPARISON_DIR=./outputs/comparisons

# Cache directory for models
CACHE_DIR=./models_cache

# =============================================================================
# DEFAULT FILE NAMES
# =============================================================================

# Default person image for demos
DEFAULT_PERSON_IMAGE=person1.jpg

# Default garment image for demos
DEFAULT_GARMENT_IMAGE=shirt1.jpg

# Default person description for demos
DEFAULT_PERSON_DESCRIPTION=person wearing a white t-shirt

# Default output filename pattern
OUTPUT_FILENAME_PATTERN={person_name}_wearing_{garment_name}

# =============================================================================
# PROCESSING OPTIONS
# =============================================================================

# Save analysis results by default
SAVE_ANALYSIS=true

# Show analysis summary by default
SHOW_SUMMARY=true

# Create comparison grids by default
CREATE_COMPARISON=true

# =============================================================================
# LOGGING AND DEBUG
# =============================================================================

# Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Enable detailed logging
DETAILED_LOGGING=false

# Save intermediate results for debugging
SAVE_INTERMEDIATE_RESULTS=false

# =============================================================================
# PERFORMANCE SETTINGS
# =============================================================================

# Enable memory optimization for Qwen
ENABLE_MEMORY_OPTIMIZATION=true

# Number of CPU threads for processing
NUM_THREADS=4

# =============================================================================
# QUALITY SETTINGS
# =============================================================================

# High quality mode (slower but better results)
HIGH_QUALITY_MODE=false

# Enable detailed prompting for better results
ENABLE_DETAILED_PROMPTING=true

# =============================================================================
# API KEYS (if needed for future extensions)
# =============================================================================

# Hugging Face token (for accessing gated models)
HUGGINGFACE_TOKEN=

# OpenAI API key (if integrating other models)
OPENAI_API_KEY=

# =============================================================================
# EXPERIMENTAL FEATURES
# =============================================================================

# Enable experimental features
ENABLE_EXPERIMENTAL=false

# Enable advanced prompting techniques
USE_ADVANCED_PROMPTING=false